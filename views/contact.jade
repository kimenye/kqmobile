#contactsPage(data-role='page')
	!=partial('header', { subtitle: 'Contact' })
	div(data-role='content')
		div(data-role='controlgroup', data-type='horizontal')
			a(data-role="button") Sales &amp; Ticketting
			a(data-role="button") Contact Center
		br
		ul(data-role='listview')
			-each location in locations
				!=partial('location', { location: location})
#maps(data-role='page')
	!=partial('header', { subtitle: 'Maps' })