#contactsPage.interior(data-role='page', data-add-back-btn="true", data-back-btn-text="Back")
	!=partial('header_nologo', { subtitle: 'Contact' })
	h4=contact.name
	p
		strong Tel:
		span
			a(href="tel:#{contact.tel1}")#{contact.tel1}
	p
		strong #{contact.tel2type} :
		span
			a(href="tel:#{contact.tel2}")#{contact.tel2}
	p
		strong Email:
		span
			a(href="mailto:#{contact.email}")#{contact.email}